{
    "title": [
        {"command": "title", "arguments": true},
        {"command": "author", "arguments": true},
        {"command": "date", "arguments": true}
    ],
    "struct": [
        {"command": "chapter", "arguments": true},
        {"command": "section", "arguments": true},
        {"command": "subsection", "arguments": true},
        {"command": "subsubsection", "arguments": true}
    ],
    "env": [
        {
            "command": "begin",
            "counterpart": "end",
            "arguments": [
                "equation",
                "subequations",
                "itemize"
            ]
        }
    ],
    "list": [
        {"command": "item"}
    ],
    "pkg": [
        {
            "command": "usepackage",
            "arguments": [
                "hyperref",
                "ansmath",
                "empheq",
                "mathtools",
                {
                    "argument": "babel",
                    "modifiers": [ "german", "english" ]
                },
                {
                    "argument": "geometry",
                    "modifiers": [ "margin=${1}" ]
                }
            ]
        },
        {
            "command": "documentclass",
            "arguments": [
                "article",
                "report"
            ],
            "modifiers": [
                "a4paper",
                "letter"
            ]
        }
    ]
}
